    "colors": "^1.1.2",
    "date-format": "0.0.2",
    "del": "^2.0.2",
    "filepaths": "^0.2.0",
    "fs-extra": "^0.26.2",
    "gulp": "^3.9.0",
    "gulp-callback": "0.0.3",
    "gulp-cheerio": "^0.6.2",
    "gulp-clean": "^0.3.1",
    "gulp-connect": "^2.2.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-if": "^2.0.0",
