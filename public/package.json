{
  "name": "asch-frontend",
  "description": "asch frontend",
  "version": "1.3.6",
  "private": true,
  "scripts": {
    "postinstall": "npm run build && node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'bower_components', 'junction') } catch (e) { }\"",
    "preinstall": "bower install",
    "start": "gulp",
    "build": "browserify build.js -o node_modules/@bower_components/bitcore-mnemonic.js && cp node_modules/asch-js/browserify-asch.js node_modules/@bower_components/ && cp node_modules/bignumber.js/bignumber.min.js node_modules/@bower_components/"
  },
